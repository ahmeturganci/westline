
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Westline | Admin Panel</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <link rel="stylesheet" href="~/Content/Admin/dist/css/AdminLTE.min.css">
    <link rel="stylesheet" href="~/Content/Admin/dist/css/skins/skin-blue.min.css">
    <script src="https://code.jquery.com/jquery-3.1.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>

    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/angular-route.min.js"></script>
    <script src="~/Content/Admin/dist/js/app.min.js"></script>
    <script src="~/Scripts/yonetim.js"></script>

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            <a href="#" class="logo">
                <span class="logo-mini"><b>W</b>L</span>
                <span class="logo-lg"><b>WEST</b>LINE</span>
            </a>
            <nav class="navbar navbar-static-top" role="navigation">
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <li class="dropdown messages-menu">
                        <li>
                            <a href="#" data-toggle="control-sidebar"><i class="fa fa-gears"></i></a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar" style="height:100%;">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel (optional) -->
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="http://www.dombilik.com/wp-content/uploads/2012/11/dedeye-yard%C4%B1m.jpg" class="img-circle" alt="User Image">
                    </div>
                    <div class="pull-left info">
                        <p>Admin Adı Çekilecek</p>
                    </div>
                </div>
                <!-- search form (Optional) -->
                <form action="#" method="get" class="sidebar-form">
                    <div class="input-group">
                        <input type="text" name="q" class="form-control" placeholder="Arama...">
                        <span class="input-group-btn">
                            <button type="submit" name="search" id="search-btn" class="btn btn-flat">
                                <i class="fa fa-search"></i>
                            </button>
                        </span>
                    </div>
                </form>
                <!-- /.search form -->
                <!-- Sidebar Menu -->
                <ul class="sidebar-menu">
                    <li class="header">Başlıklar</li>
                    <!-- Optionally, you can add icons to the links -->
                    <li class="active"><a href="#"><i class="fa fa-link"></i> <span>sayfa 1 </span></a></li>
                    <li>
                       <li> <a href="~/YonetimPanel/profilInput"><i class="fa fa-link"></i> <span>PROFİL SAYFASI INPUT</span></a></li>
                    <li> <a href="~/YonetimPanel/CvInput"><i class="fa fa-link"></i> <span>CV SAYFASI INPUT</span></a></li>
                    <li> <a href="~/YonetimPanel/IslerInput"><i class="fa fa-link"></i> <span>İŞLER SAYFASI INPUT</span></a></li>
                    <li> <a href="~/YonetimPanel/UcakBilgiInput"><i class="fa fa-link"></i> <span>UCAK BİLGİ SAYFASI INPUT</span></a></li>
                    <li> <a href="~/YonetimPanel/AktivasyonInput"><i class="fa fa-link"></i> <span>AKTİVASYON SAYFASI INPUT</span></a></li>
                    <li> <a href="~/YonetimPanel/BirinciSayfaInput"><i class="fa fa-link"></i> <span>BİRİNCİ SAYFA INPUT</span></a></li>
                    <li> <a href="~/YonetimPanel/IkinciSayfaInput"><i class="fa fa-link"></i> <span>İKİNCİ SAYFA INPUT</span></a></li>
                    <li> <a href="~/YonetimPanel/UcuncuSayfaInput"><i class="fa fa-link"></i> <span>UCUNCU SAYFA INPUT</span></a></li>
                    <li> <a href="~/YonetimPanel/DorduncuSayfaInput"><i class="fa fa-link"></i> <span>DORDUNCU SAYFA INPUT</span></a></li>
                    <li> <a href="~/YonetimPanel/BesinciSayfaInput"><i class="fa fa-link"></i> <span>BESİNCİ SAYFA INPUT</span></a></li>

                </ul>
                <!-- /.sidebar-menu -->
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    Westline Admin Paneli
                    <small>Vallaha bilemedim ki </small>
                </h1>
            </section>
            <!-- Main content -->
            <section class="content" style="padding-bottom:700px">
                @RenderBody()
            </section>
        </div>
        <!-- /.content-wrapper -->
        <!-- Main Footer -->
        <footer class="main-footer">
            <!-- To the right -->
            <div class="pull-right hidden-xs">
                erahok yazılım geliştirme takımı
            </div>
            <!-- Default to the left -->
            <strong>Copyright &copy; 2016 <a href="http://www.github.com/ahmeturganci/westline">Westline</a>.</strong> Tüm hakları zaten bizde.
        </footer>
        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Create the tabs -->
            <ul class="nav nav-tabs nav-justified control-sidebar-tabs">
                <li class="active"><a href="#control-sidebar-home-tab" data-toggle="tab"><i class="fa fa-home"></i></a></li>
                <li><a href="#control-sidebar-settings-tab" data-toggle="tab"><i class="fa fa-gears"></i></a></li>
            </ul>
            <!-- Tab panes -->
            <div class="tab-content">
                <div class="tab-pane active" id="control-sidebar-home-tab">
                    <h3 class="control-sidebar-heading">Profil Bilgisi</h3>
                </div>
                <div class="tab-pane" id="control-sidebar-settings-tab">
                    <h3 class="control-sidebar-heading">Ayarlar</h3>
                </div>
            </div>
        </aside>

        <div class="control-sidebar-bg"></div>
    </div>
</body>
</html>
